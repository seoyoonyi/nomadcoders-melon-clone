{
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"parserOptions": {
		"ecmaVersion": 2021,
		"sourceType": "module"
	},
	"extends": ["eslint:recommended"],
	"rules": {
		"semi": ["error", "always"],
		"quotes": ["error", "single"],
		"no-console": "warn"
	},
	"globals": {
		"YT": "readonly"
	}
}
